{
  "name": "Select Layers",
  "description": "A Sketch plugin for changing or creating a selection of layers based on name, type or similarity",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-select-layers",
  "version": "0.2.1",
  "identifier": "yuanqing.sketch-select-layers",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-select-layers/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Select By Name",
      "identifier": "yuanqing.sketch-select-layers.select-by-name",
      "script": "plugin.js",
      "handler": "select-by-name"
    },
    {
      "name": "Select Artboards",
      "identifier": "yuanqing.sketch-select-layers.select-artboards",
      "script": "plugin.js",
      "handler": "select-by-type/select-artboards"
    },
    {
      "name": "Select Groups",
      "identifier": "yuanqing.sketch-select-layers.select-groups",
      "script": "plugin.js",
      "handler": "select-by-type/select-groups"
    },
    {
      "name": "Select Text Layers",
      "identifier": "yuanqing.sketch-select-layers.select-text-layers",
      "script": "plugin.js",
      "handler": "select-by-type/select-text-layers"
    },
    {
      "name": "Select Shape Layers",
      "identifier": "yuanqing.sketch-select-layers.select-shape-layers",
      "script": "plugin.js",
      "handler": "select-by-type/select-shape-layers"
    },
    {
      "name": "Select Symbol Instances",
      "identifier": "yuanqing.sketch-select-layers.select-symbol-instances",
      "script": "plugin.js",
      "handler": "select-by-type/select-symbol-instances"
    },
    {
      "name": "Select Images",
      "identifier": "yuanqing.sketch-select-layers.select-images",
      "script": "plugin.js",
      "handler": "select-by-type/select-images"
    },
    {
      "name": "Select Slices",
      "identifier": "yuanqing.sketch-select-layers.select-slices",
      "script": "plugin.js",
      "handler": "select-by-type/select-slices"
    },
    {
      "name": "Select Hotspots",
      "identifier": "yuanqing.sketch-select-layers.select-hotspots",
      "script": "plugin.js",
      "handler": "select-by-type/select-hotspots"
    },
    {
      "name": "Select Hidden Layers",
      "identifier": "yuanqing.sketch-select-layers.select-hidden-layers",
      "script": "plugin.js",
      "handler": "select-by-type/select-hidden-layers"
    },
    {
      "name": "Select Same Name",
      "identifier": "yuanqing.sketch-select-layers.select-same-name",
      "script": "plugin.js",
      "handler": "select-same/select-same-name"
    },
    {
      "name": "Select Same Layer Style or Text Style",
      "identifier": "yuanqing.sketch-select-layers.select-same-layer-style-or-text-style",
      "script": "plugin.js",
      "handler": "select-same/select-same-style"
    },
    {
      "name": "Select Same Symbol Instance",
      "identifier": "yuanqing.sketch-select-layers.select-same-symbol-instance",
      "script": "plugin.js",
      "handler": "select-same/select-same-symbol-instance"
    }
  ],
  "menu": {
    "title": "Select Layers",
    "items": [
      "yuanqing.sketch-select-layers.select-by-name",
      "-",
      "yuanqing.sketch-select-layers.select-artboards",
      "yuanqing.sketch-select-layers.select-groups",
      "yuanqing.sketch-select-layers.select-text-layers",
      "yuanqing.sketch-select-layers.select-shape-layers",
      "yuanqing.sketch-select-layers.select-symbol-instances",
      "yuanqing.sketch-select-layers.select-images",
      "yuanqing.sketch-select-layers.select-slices",
      "yuanqing.sketch-select-layers.select-hotspots",
      "yuanqing.sketch-select-layers.select-hidden-layers",
      "-",
      "yuanqing.sketch-select-layers.select-same-name",
      "yuanqing.sketch-select-layers.select-same-layer-style-or-text-style",
      "yuanqing.sketch-select-layers.select-same-symbol-instance"
    ]
  }
}
