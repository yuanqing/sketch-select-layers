{
  "name": "Select Layers",
  "description": "A Sketch plugin to create or filter a selection of layers based on name, type or similarity",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-select-layers",
  "version": "0.6.0",
  "identifier": "yuanqing.sketch-select-layers",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-select-layers/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Select By Name",
      "identifier": "yuanqing.sketch-select-layers.select-by-name",
      "script": "plugin.js",
      "handler": "select-by-name"
    },
    {
      "name": "Select Artboards",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.artboard",
      "script": "plugin.js",
      "handler": "select-by-type/types/artboard"
    },
    {
      "name": "Select Groups",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.group",
      "script": "plugin.js",
      "handler": "select-by-type/types/group"
    },
    {
      "name": "Select Shape Layers",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.shape-layer",
      "script": "plugin.js",
      "handler": "select-by-type/types/shape-layer"
    },
    {
      "name": "Select Text Layers",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.text-layer",
      "script": "plugin.js",
      "handler": "select-by-type/types/text-layer"
    },
    {
      "name": "Select Symbol Instances",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.symbol-instance",
      "script": "plugin.js",
      "handler": "select-by-type/types/symbol-instance"
    },
    {
      "name": "Select Images",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.image",
      "script": "plugin.js",
      "handler": "select-by-type/types/image"
    },
    {
      "name": "Select Slices",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.slice",
      "script": "plugin.js",
      "handler": "select-by-type/types/slice"
    },
    {
      "name": "Select Hotspots",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.hotspot",
      "script": "plugin.js",
      "handler": "select-by-type/types/hotspot"
    },
    {
      "name": "Select Hidden Layers",
      "identifier": "yuanqing.sketch-select-layers.select-by-type.types.hidden-layer",
      "script": "plugin.js",
      "handler": "select-by-type/types/hidden-layer"
    },
    {
      "name": "Select Same Name",
      "identifier": "yuanqing.sketch-select-layers.select-same.attributes.name",
      "script": "plugin.js",
      "handler": "select-same/attributes/name"
    },
    {
      "name": "Select Same Layer Style or Text Style",
      "identifier": "yuanqing.sketch-select-layers.select-same.attributes.style",
      "script": "plugin.js",
      "handler": "select-same/attributes/style"
    },
    {
      "name": "Select Same Symbol Instance",
      "identifier": "yuanqing.sketch-select-layers.select-same.attributes.symbol-instance",
      "script": "plugin.js",
      "handler": "select-same/attributes/symbol-instance"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-select-layers.reset-settings",
      "script": "plugin.js",
      "handler": "reset-settings"
    }
  ],
  "menu": {
    "title": "Select Layers",
    "items": [
      "yuanqing.sketch-select-layers.select-by-name",
      "-",
      "yuanqing.sketch-select-layers.select-by-type.types.artboard",
      "yuanqing.sketch-select-layers.select-by-type.types.group",
      "yuanqing.sketch-select-layers.select-by-type.types.shape-layer",
      "yuanqing.sketch-select-layers.select-by-type.types.text-layer",
      "yuanqing.sketch-select-layers.select-by-type.types.symbol-instance",
      "yuanqing.sketch-select-layers.select-by-type.types.image",
      "yuanqing.sketch-select-layers.select-by-type.types.slice",
      "yuanqing.sketch-select-layers.select-by-type.types.hotspot",
      "yuanqing.sketch-select-layers.select-by-type.types.hidden-layer",
      "-",
      "yuanqing.sketch-select-layers.select-same.attributes.name",
      "yuanqing.sketch-select-layers.select-same.attributes.style",
      "yuanqing.sketch-select-layers.select-same.attributes.symbol-instance",
      "-",
      "yuanqing.sketch-select-layers.reset-settings"
    ]
  }
}
